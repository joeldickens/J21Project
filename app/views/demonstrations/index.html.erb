<div class="container">
  <h1>Demonstrations</h1>
  <h4>In progress</h4>
</div>

<div class="causes-container">
  <% @demonstrations.each do |demonstration| %>
    <% if demonstration.name != '' && demonstration.name != nil %>
      <div class="card">
        <h5><%= link_to demonstration.name, demonstration.website %></h5>
      </div>
    <% end %>
  <% end %>
</div>
